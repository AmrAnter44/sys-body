    "csv-parse": "^6.1.0",
    "electron-is-dev": "^2.0.0",
    "exceljs": "^4.4.0",
    "html5-qrcode": "^2.3.8",
    "jsonwebtoken": "^9.0.2",
    "jsqr": "^1.4.0",
    "next": "^14.2.33",
