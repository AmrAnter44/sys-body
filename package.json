{
  "name": "gym-management",
  "version": "5.8.1",
  "description": "gym system",
  "private": true,
  "main": "electron/main.js",
  "author": "Amr Anter",
  "scripts": {
    "dev": "node server.js",
    "dev:turbo": "node --max-old-space-size=4096 node_modules/next/dist/bin/next dev --turbo -p 4001 -H 0.0.0.0",
    "dev:default": "next dev -p 4001 -H 0.0.0.0",
    "build": "prisma generate && next build && npm run postbuild",
    "postbuild": "node postbuild.js",
    "start": "next start -p 4001 -H 0.0.0.0",
    "postinstall": "prisma generate",
    "db:push": "prisma db push",
    "db:optimize": "node optimize-database.js",
    "db:studio": "prisma studio",
    "generate:pwa-icons": "node generate-pwa-icons.js",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:4001 --timeout 180000 --interval 1000 --httpTimeout 10000 && electron . --enable-features=WebRTC --enable-usermedia-screen-capturing\"",
    "electron:dev:alt": "concurrently \"npm run dev\" \"node start-electron.js\"",
    "electron:build": "electron-builder --publish never",
    "electron:build:win": "electron-builder --win --x64 --publish never"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^18.0.1",
    "@next/env": "^16.0.3",
    "@prisma/client": "^5.22.0",
    "@swc/helpers": "^0.5.17",
    "@tanstack/react-query": "^5.90.19",
    "@tanstack/react-query-devtools": "^5.91.2",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.3",
    "better-sqlite3": "^12.5.0",
    "bwip-js": "^4.8.0",
    "client-only": "^0.0.1",
    "csv-parse": "^6.1.0",
    "dom-to-image-more": "^3.7.2",
    "electron-is-dev": "^2.0.0",
    "electron-updater": "^6.7.3",
    "exceljs": "^4.4.0",
    "expo-server-sdk": "^5.0.0",
    "html2canvas": "^1.4.1",
    "html5-qrcode": "^2.3.8",
    "http-proxy": "^1.18.1",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^4.0.0",
    "jspdf-autotable": "^5.0.7",
    "jsqr": "^1.4.0",
    "next": "^14.2.33",
    "next-pwa": "^5.6.0",
    "node-hid": "^3.2.0",
    "postcss": "^8.5.6",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^3.5.1",
    "styled-jsx": "^5.1.7",
    "uiohook-napi": "^1.5.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.0.0",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "concurrently": "^8.2.2",
    "electron": "^39.2.3",
    "electron-builder": "^24.13.3",
    "prisma": "^5.22.0",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.0.0",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.gym.management",
    "productName": "Gym Management",
    "directories": {
      "output": "dist"
    },
    "files": [
      "electron/**/*",
      ".next/standalone/**/*",
      "!.next/standalone/node_modules",
      ".next/static/**/*",
      "build/**/*",
      "prisma/**/*",
      "package.json"
    ],
    "asarUnpack": [
      ".next/standalone/**/*",
      "!.next/standalone/node_modules",
      "node_modules/uiohook-napi/**/*",
      "node_modules/better-sqlite3/**/*",
      "node_modules/node-hid/**/*",
      "electron/standalone-server.js"
    ],
    "extraResources": [
      {
        "from": ".next/standalone/node_modules",
        "to": "app.asar.unpacked/.next/standalone/node_modules",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "prisma/gym.db",
        "to": "seed-database/gym.db"
      }
    ],
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "build/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "installerIcon": "build/icon.ico",
      "uninstallerIcon": "build/icon.ico"
    },
    "publish": {
      "provider": "github",
      "owner": "AmrAnter44",
      "repo": "sys-Xgym"
    }
  }
}
