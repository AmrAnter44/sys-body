{
  "name": "gym-management",
  "version": "1.0.0",
  "description": "xgym system",
  "private": true,
  "main": "electron/main.js",
  "author": "Amr Anter",
  "scripts": {
    "dev": "next dev -p 4001 -H 0.0.0.0",
